            <VBox prefHeight="170.0" prefWidth="600.0" style="-fx-background-color: #fff;">
               <children>
                  <Label fx:id="question" alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" prefHeight="73.0" prefWidth="600.0" text="&lt; QUESTION PLACEHOLDER &gt;" textAlignment="CENTER" textFill="#515151" textOverrun="WORD_ELLIPSIS" wrapText="true">
